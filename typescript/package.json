{
  "name": "@k256/sdk",
  "version": "0.1.5",
  "description": "Official TypeScript SDK for K256 - the gateway to Solana's liquidity ecosystem",
  "author": "K256 <support@k256.xyz>",
  "license": "MIT",
  "homepage": "https://k256.xyz",
  "repository": {
    "type": "git",
    "url": "https://github.com/k256-xyz/k256-sdks",
    "directory": "typescript"
  },
  "bugs": {
    "url": "https://github.com/k256-xyz/k256-sdks/issues"
  },
  "keywords": [
    "solana",
    "swap",
    "dex",
    "aggregator",
    "k256",
    "websocket",
    "defi",
    "trading"
  ],
  "type": "module",
  "main": "./dist/index.cjs",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "import": {
        "types": "./dist/index.d.ts",
        "default": "./dist/index.js"
      },
      "require": {
        "types": "./dist/index.d.cts",
        "default": "./dist/index.cjs"
      }
    },
    "./ws": {
      "import": {
        "types": "./dist/ws/index.d.ts",
        "default": "./dist/ws/index.js"
      },
      "require": {
        "types": "./dist/ws/index.d.cts",
        "default": "./dist/ws/index.cjs"
      }
    },
    "./types": {
      "import": {
        "types": "./dist/types/index.d.ts",
        "default": "./dist/types/index.js"
      },
      "require": {
        "types": "./dist/types/index.d.cts",
        "default": "./dist/types/index.cjs"
      }
    },
    "./utils": {
      "import": {
        "types": "./dist/utils/index.d.ts",
        "default": "./dist/utils/index.js"
      },
      "require": {
        "types": "./dist/utils/index.d.cts",
        "default": "./dist/utils/index.cjs"
      }
    }
  },
  "files": [
    "dist",
    "src",
    "README.md",
    "LICENSE"
  ],
  "scripts": {
    "build": "tsup",
    "dev": "tsup --watch",
    "typecheck": "tsc --noEmit",
    "lint": "eslint src/",
    "prepublishOnly": "npm run build"
  },
  "devDependencies": {
    "tsup": "^8.0.0",
    "typescript": "^5.3.0"
  },
  "engines": {
    "node": ">=18.0.0"
  },
  "sideEffects": false
}
